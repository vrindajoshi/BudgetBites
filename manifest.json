{
    "manifest_version": 3,
    "name": "BudgetBites",
    "description": "",
    "version": "1.0",
    "icons": { 
        "16": "/images/logo - 16.png",
        "48": "/images/logo - 48.png",
        "128": "/images/logo - 128.png"
    },
    "background": {
        "service_worker": "service-worker.js",
        "type": "module"
    },
    "content_scripts": [ {
        "matches": ["https://www.walmart.ca/en/cp/grocery/*"],
        "js": ["script.js"]
      } ],
    "action": {
        "default_popup": "index.html"
    }
}